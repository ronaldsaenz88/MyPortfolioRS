<!DOCTYPE html>
<html>
  <head>
    <%- include('partials/head'); %>
  </head>
  <body id="page-top">
    <!-- Begin Preloader -->
    <%- include('partials/preloader'); %>
    <!-- End Preloader -->
    <div class="page db-social">
      <!-- Begin Header -->
      <%- include('partials/header'); %>
      <!-- End Header -->

      <!-- Begin Page Content -->
      <div class="page-content d-flex align-items-stretch">
        <!-- Begin Side Navbar -->
        <div class="compact-sidebar light-sidebar has-shadow">
          <%- include('partials/sidebar'); %>
        </div>
        <!-- End Side Navbar -->
        
        <!-- Begin Content -->
        <div class="content-inner compact">
          <div class="container-fluid">
            <div class="row justify-content-center">
                <div class="col-xl-11">
                    <!-- Begin Page Header-->
                    <div class="page-header pr-0 pl-0">
                        <div class="d-flex align-items-center">
                            <h2 class="page-header-title">Contact</h2>
                            <div>
                                <ul class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="mailto: rsaenzh@my.centennialcollege.ca"><i class="ti ti-email"></i></a></li>
                                    <li class="breadcrumb-item"><a href="mailto: ronaldsaenz88@gmail.com"><i class="ti ti-email"></i></a></li>
                                    <li class="breadcrumb-item"><a href="https://www.linkedin.com/in/ronald-saenz/?locale=en_US" target="_blank"><i class="ti ti-linkedin"></i></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <!-- End Page Header -->
                    <div class="row flex-row">
                        <div class="col-xl-12">
                            <!-- Form -->
                            <div class="widget has-shadow">
                                <div class="widget-header bordered no-actions d-flex align-items-center">
                                    <h4>Contact Me</h4>
                                </div>
                                <div class="widget-body">
                                    <form class="needs-validation" novalidate>
                                        <div class="form-group row d-flex align-items-center mb-5">
                                            <label class="col-lg-4 form-control-label d-flex justify-content-lg-end">First Name</label>
                                            <div class="col-lg-5">
                                                <input name="first_name" id="first_name" type="text" class="form-control" placeholder="Enter your first name">
                                            </div>
                                        </div>
                                        <div class="form-group row d-flex align-items-center mb-5">
                                            <label class="col-lg-4 form-control-label d-flex justify-content-lg-end">Last Name</label>
                                            <div class="col-lg-5">
                                                <input name="last_name" id="last_name" type="text" class="form-control" placeholder="Enter your last name">
                                            </div>
                                        </div>
                                        <div class="form-group row d-flex align-items-center mb-5">
                                            <label class="col-lg-4 form-control-label d-flex justify-content-lg-end">Email Address *</label>
                                            <div class="col-lg-5">
                                                <div class="input-group">
                                                    <input name="email" id="email" type="email" class="form-control" placeholder="Enter your email" required>
                                                    <span class="input-group-addon addon-orange">.com</span>
                                                    <div class="invalid-feedback">
                                                        Please enter a valid email
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group row d-flex align-items-center mb-5">
                                            <label class="col-lg-4 form-control-label d-flex justify-content-lg-end">Phone Number</label>
                                            <div class="col-lg-5">
                                                <div class="input-group">
                                                    <span class="input-group-addon addon-primary">
                                                        <i class="la la-phone"></i>
                                                    </span>
                                                    <input name="phone_number" id="phone_number" type="text" class="form-control" placeholder="Phone number">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group row d-flex align-items-center mb-5">
                                            <label class="col-lg-4 form-control-label d-flex justify-content-lg-end">Message</label>
                                            <div class="col-lg-5">
                                                <textarea name="message" id="message" class="form-control" placeholder="Type your message here ..." required></textarea>
                                                <div class="invalid-feedback">
                                                    Please enter a custom message
                                                </div>
                                            </div>
                                        </div>
                                        <div class="em-separator separator-dashed"></div>
                                        <div class="text-right">
                                            <button class="btn btn-gradient-01" type="submit">Submit Form</button>
                                            <button class="btn btn-shadow" type="reset">Reset</button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <!-- End Form -->
                        </div>
                    </div>
                    <!-- End Row -->
                </div>
              <!-- End Col -->
            </div>
            <!-- End Row -->
          </div>
          <!-- End Container -->

          <!-- Begin Page Footer-->
          <%- include('partials/footer'); %>
          <!-- End Page Footer -->
        </div>
        <!-- End Content -->
      </div>
      <!-- End Page Content -->
    </div>

    <!-- Begin Vendor Js and Page Vendor Js -->
    <%- include('partials/foot_js'); %>
    <!-- End Vendor Js and Page Vendor Js -->
  </body>
</html>
